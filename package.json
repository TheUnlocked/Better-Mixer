    "eslint": "^6.6.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prefer-arrow": "^1.1.6",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "typescript": "^3.7.2",
    "web-ext": "^3.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheUnlocked/Better-Mixer.git"
