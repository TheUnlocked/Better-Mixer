{
  "name": "better-mixer",
  "version": "3.1.3",
  "description": "Improvements to Mixer, similar to FFZ for Twitch.",
  "scripts": {
    "lint": "eslint -c .eslintrc ./Extension/**/*.{ts,js}",
    "lint:fix": "eslint -c .eslintrc ./Extension/**/*.{ts,js} --fix",
    "serve:firefox": "web-ext run --source-dir ./build/",
    "package": "npm run build && web-ext build --source-dir ./build/",
    "clean": "gulp clean",
    "build:dev": "tsc --inlineSourceMap --inlineSources --sourceRoot \"/\" && gulp",
    "build": "npm run clean && tsc && gulp",
    "watch": "concurrently --raw \"gulp watch\" \"tsc --inlineSourceMap --inlineSources --sourceRoot \"/\" --watch\""
  },
  "devDependencies": {
    "@types/chrome": "0.0.91",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.1.0",
    "del": "^5.1.0",
    "eslint": "^6.6.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prefer-arrow": "^1.1.6",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-replace": "^1.0.0",
    "typescript": "^3.8.3",
    "web-ext": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheUnlocked/Better-Mixer.git"
  },
  "author": "Unlocked",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheUnlocked/Better-Mixer/issues"
  },
  "homepage": "https://github.com/TheUnlocked/Better-Mixer"
}
